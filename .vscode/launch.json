{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/Server/main.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/Client",
      "runtimeArgs": [
        "--preserve-symlinks"
      ]
    }
  ]
}